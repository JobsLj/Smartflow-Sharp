@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    <meta name="viewport" content="width=device-width" />
    <title>Smartflow 业务流程图</title>
    <link href="~/Content/smartflow.css" rel="stylesheet" />
    <script src="~/Content/jquery.min.js"></script>
    <script src="~/Content/json2.js"></script>
    <script src="~/Content/svg.js"></script>
    <script src="~/Content/utils.js"></script>
    <script src="~/Content/smartflow.js"></script>
    <script type="text/javascript">
        $(function () {
            var draw = window.SMF.init({container:'drawing'}),
                    serverData=@Html.Raw(ViewBag.Result),
                    processData=@(ViewBag.Record==null?"''":Html.Raw(ViewBag.Record));
            if (serverData) {
                SMF.revert(serverData.structure, true, serverData.id,processData);
            }
        })
    </script>
</head>
<body>
    <div class="container">
        <div class="container-draw" id="drawing"></div>
    </div>
</body>
</html>
